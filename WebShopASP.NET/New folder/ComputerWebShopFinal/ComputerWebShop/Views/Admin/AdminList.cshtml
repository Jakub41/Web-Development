@model IEnumerable<ComputerWebShop.ViewModel.AddAdminViewModel>

@{
    ViewBag.Title = "AdminList";
    Layout = "~/Views/Shared/LayoutAdmin.cshtml";
}


<p>
    @Html.ActionLink("Create Admin", "AddAdmin")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
       
        <th>
            @Html.DisplayNameFor(model => model.RoleType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.RoleType)
        </td>
        <td>
            
            @Html.ActionLink("Delete", "DeleteAdmin", new { id = item.ID })
        </td>
    </tr>
}

</table>
